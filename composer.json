{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.5",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.6",
        "jms/serializer": "^3.32",
        "liip/imagine-bundle": "^2.15",
        "nelmio/cors-bundle": "^2.6",
        "nesbot/carbon": "^3.11",
        "phpdocumentor/reflection-docblock": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/expression-language": "^8.0",
        "symfony/finder": "^8.0",
        "symfony/flex": "^2.4",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/ldap": "^8.0",
        "symfony/lock": "^8.0",
        "symfony/mailer": "^8.0",
        "symfony/monolog-bundle": "^v4.0",
        "symfony/process": "^8.0",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/rate-limiter": "^8.0",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/validator": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/webpack-encore-bundle": "^2.4",
        "symfony/yaml": "^8.0",
        "willdurand/hateoas": "^3.14"
    },
    "require-dev": {
        "symfony/browser-kit": "^8.0",
        "symfony/config": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/maker-bundle": "^1.65",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "kylekatarnls/update-helper": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C6ZMDTZYHN7CSWAEMWXD67J8",
            "allow-contrib": true
        }
    }
}
